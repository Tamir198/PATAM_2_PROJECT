<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="353.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <TextField fx:id="date" alignment="CENTER" layoutX="59.0" layoutY="41.0" promptText="Date">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="starting_time" alignment="CENTER" layoutX="59.0" layoutY="78.0" promptText="Starting Time">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="landing_time" alignment="CENTER" layoutX="59.0" layoutY="115.0" promptText="Landing Time">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <ComboBox fx:id="currently_flying" layoutX="60.0" layoutY="154.0" prefHeight="25.0" prefWidth="173.0" promptText="Currently Flying" />
      <Button alignment="CENTER" layoutX="24.0" layoutY="284.0" mnemonicParsing="false" onAction="#getFieldValues" prefHeight="40.0" prefWidth="121.0" text="Add Flight" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
      <TextField fx:id="max_speed" alignment="CENTER" layoutX="59.0" layoutY="223.0" promptText="Max Speed">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="max_height" alignment="CENTER" layoutX="59.0" layoutY="186.0" promptText="Max Height">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label fx:id="status" layoutX="59.0" layoutY="14.0" prefHeight="2.0" prefWidth="184.0" textFill="#eb2020">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="att" alignment="CENTER" layoutX="148.0" layoutY="284.0" mnemonicParsing="false" onAction="#goToFlightList" prefHeight="40.0" prefWidth="121.0" text="Flight List" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
      
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</Pane>
